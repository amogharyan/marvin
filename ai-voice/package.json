{
  "name": "dev2-ai-voice",
  "version": "1.0.0",
  "description": "Dev 2 AI & Voice Integration Service for Marvin AR Assistant",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:services": "jest tests/services",
    "lint": "eslint src/**/*.ts tests/**/*.ts --ext .ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"const fs = require('fs'); if (fs.existsSync('dist')) fs.rmSync('dist', { recursive: true, force: true });\"",
    "tdd": "jest --watch --verbose"
  },
  "keywords": [
    "ai",
    "voice",
    "ar",
    "assistant",
    "gemini",
    "elevenlabs",
    "eigenvalue"
  ],
  "author": "Dev 2 - AI & Voice Integration",
  "license": "ISC",
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.20.1",
    "@google/generative-ai": "^0.24.1",
    "@letta-ai/letta-client": "^0.0.68665",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/node": "^24.9.1",
    "axios": "^1.12.2",
    "chromadb": "^3.0.17",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "elevenlabs": "^1.59.0",
    "express": "^5.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5"
  }
}
