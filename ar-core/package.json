{
  "name": "marvin-ar-core",
  "version": "1.0.0",
  "description": "AR Core implementation for Marvin Morning Assistant using Snap Spectacles",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint tests/**/*.ts || echo 'No TypeScript files to lint'",
    "lint:fix": "eslint tests/**/*.ts --fix || echo 'No TypeScript files to lint'",
    "clean": "node -e \"const fs = require('fs'); if (fs.existsSync('dist')) fs.rmSync('dist', { recursive: true, force: true });\"",
    "prebuild": "npm run clean",
    "setup-lens-studio": "./setup-lens-studio.sh"
  },
  "keywords": [
    "ar",
    "snap-spectacles",
    "lens-studio",
    "object-detection",
    "computer-vision"
  ],
  "author": "Marvin AR Team - Dev 1",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marvin-ar/marvin-ar-assistant"
  }
}
