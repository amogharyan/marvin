--allman false
--indent 4
--indentcase false
--ifdef outdent
--extensionacl on-declarations
--self init-only
--semicolons never
--commas always
--wrapparameters before-first
--wraparguments before-first
--wrapcollections before-first
--wrapreturntype preserve
--wrapconditions before-first
--funcattributes prev-line
--typeattributes prev-line
--storedvarattrs same-line
--computedvarattrs same-line
--yodaswap always
--maxwidth 120
--swiftversion 5.7
--rules \
  andOperator, \
  anyObjectProtocol, \
  blankLinesAroundMark, \
  blankLinesAtEndOfScope, \
  blankLinesAtStartOfScope, \
  braces, \
  consecutiveBlankLines, \
  consecutiveSpaces, \
  duplicateImports, \
  elseOnSameLine, \
  enumNamespaces, \
  extensionAccessControl, \
  hoistPatternLet, \
  indent, \
  initCoderUnavailable, \
  leadingDelimiters, \
  linebreakAtEndOfFile, \
  modifierOrder, \
  numberFormatting, \
  preferKeyPath, \
  redundantBackticks, \
  redundantBreak, \
  redundantExtensionACL, \
  redundantFileprivate, \
  redundantGet, \
  redundantInit, \
  redundantLet, \
  redundantLetError, \
  redundantNilInit, \
  redundantObjc, \
  redundantOptionalBinding, \
  redundantParens, \
  redundantPattern, \
  redundantRawValues, \
  redundantSelf, \
  redundantType, \
  redundantVoidReturnType, \
  semicolons, \
  sortImports, \
  spaceAroundBraces, \
  spaceAroundBrackets, \
  spaceAroundComments, \
  spaceAroundGenerics, \
  spaceAroundOperators, \
  spaceAroundParens, \
  spaceInsideBraces, \
  spaceInsideBrackets, \
  spaceInsideComments, \
  spaceInsideGenerics, \
  spaceInsideParens, \
  strongifiedSelf, \
  todos, \
  trailingClosures, \
  trailingCommas, \
  trailingSpace, \
  typeSugar, \
  void, \
  wrapArguments, \
  wrapAttributes, \
  wrapEnumCases, \
  wrapMultilineStatementBraces, \
  wrapSingleLineComments, \
  yodaConditions, \
  docComments
